        </dependency>

        <dependency>
            <groupId>opennars</groupId>
            <artifactId>opennars-core</artifactId>
            <version>1.7-SNAPSHOT</version>
        </dependency>
        <dependency>
            <groupId>opennars</groupId>
            <artifactId>opennars-prolog</artifactId>
            <version>1.7-SNAPSHOT</version>
        </dependency>
        <dependency>
            <groupId>opennars</groupId>
            <artifactId>opennars-gui</artifactId>
            <version>1.7-SNAPSHOT</version>
        </dependency>        
        <dependency>
            <groupId>opennars</groupId>
            <artifactId>opennars-web</artifactId>
            <version>1.7-SNAPSHOT</version>
        </dependency>

